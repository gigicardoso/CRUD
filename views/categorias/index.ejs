<h1>Lista de Categorias</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Categorias</th>
        </tr>
    </thead>
    <tbody>
        <% categorias.forEach(categoria => { %>
            <tr>
                <td><%= categoria.id %></td>
                <td><%= categoria.nome %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
